<!-- sidebar social -->
<% if(theme.sidebar.social) { %>
    <div class="trm-divider trm-mb-40 trm-mt-40"></div>
    <div class="trm-social">
        <% theme.sidebar.social.forEach(item=>{ %>
            <a href="<%- url_for(item.url) %>" title="<%- item.name %>" rel="me" target="_blank" class="u-url">
                <%- icon(item.icon) %>
            </a>
        <%})%>
    </div>
<% } %>
<!-- sidebar social end -->